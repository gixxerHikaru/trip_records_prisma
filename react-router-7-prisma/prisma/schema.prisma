generator client {
  provider        = "prisma-client"
  output          = "../app/generated/prisma"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Prefecture {
  id   String @id @unique
  name String @unique
  area String
  TripRecords TripRecord[]
}


model TripRecord {
  id              String     @id @unique
  visitedDate     DateTime
  prefectureId    String
  visitedPrefecture Prefecture @relation(fields: [prefectureId], references: [id])
  detail          String?
}
